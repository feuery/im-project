(ns mese-test.web
  (:require [ring.adapter.jetty :as jetty]))

(defn app [req]
  {:status 200
   :headers {"Content-Type" "text/html"}
   :body "Hello <b>WORLDdsadssd!</b>"})

(defn -main [port]
  (jetty/run-jetty app {:port (Integer. port) :join? false}))

